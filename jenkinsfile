pipeline {
  agent any
  stages {
        
    stage('Git') {
      steps {
        git 'https://github.com/hakantektas/mac-webdriverIO.git'
      }
    }
     
    stage('Build') {
      steps {
        sh 'npm install'
      }
    }  
    
            
    stage('Test') {
      steps {
        sh 'npm run test'
      }
    }
  }
}